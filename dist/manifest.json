{
  "manifest_version": 3,
  "name": "DiscPost",
  "version": "1.2.1",
  "description": "Posts simple text messages to Discord channels via webhooks. Recommended to set a keyboard shortcut for quick posting.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "statics/img/icon.png"
  },
  "options_page": "setting.html",
  "host_permissions": [
    "https://discord.com/"
  ],
    "background": {
    "service_worker": "statics/js/background.js"
  }
}